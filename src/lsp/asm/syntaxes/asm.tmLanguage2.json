      "match": "\\b([aA]|[aA][cC][iI]|[aA][dD][cC]|[aA][dD][dD]|[aA][dD][iI]|[aA][nN][aA]|[aA][nN][iI]|[bB]|[cC]|[cC][aA][lL][lL]|[cC][cC]|[cC][mM]|[cC][mM][aA]|[cC][mM][cC]|[cC][mM][pP]|[cC][nN][cC]|[cC][nN][zZ]|[cC][pP]|[cC][pP][eE]|[cC][pP][iI]|[cC][pP][oO]|[cC][zZ]|[dD]|[dD][aA][dD]|[dD][bB]|[dD][cC][rR]|[dD][cC][xX]|[dD][sS]|[dD][wW]|[eE]|[eE][qQ][uU]|[hH]|[hH][lL][tT]|[iI][nN][rR]|[iI][nN][xX]|[jJ][cC]|[jJ][mM]|[jJ][mM][pP]|[jJ][nN][cC]|[jJ][nN][zZ]|[jJ][pP]|[jJ][pP][eE]|[jJ][pP][oO]|[jJ][zZ]|[lL]|[lL][dD][aA]|[lL][dD][aA][xX]|[lL][hH][lL][dD]|[lL][xX][iI]|[mM]|[mM][oO][vV]|[mM][vV][iI]|[nN][oO][pP]|[oO][rR][aA]|[oO][rR][gG]|[oO][rR][iI]|[oO][uU][tT]|[pP][cC][hH][lL]|[pP][oO][pP]|[pP][sS][wW]|[pP][uU][sS][hH]|[rR][aA][lL]|[rR][aA][rR]|[rR][cC]|[rR][eE][tT]|[rR][lL][cC]|[rR][mM]|[rR][nN][cC]|[rR][nN][zZ]|[rR][pP]|[rR][pP][eE]|[rR][pP][oO]|[rR][rR][cC]|[rR][zZ]|[sS][bB][bB]|[sS][bB][iI]|[sS][hH][lL][dD]|[sS][pP]|[sS][tT][aA]|[sS][tT][aA][xX]|[sS][tT][cC]|[sS][uU][bB]|[sS][uU][iI]|[xX][cC][hH][gG]|[xX][rR][aA]|[xX][rR][iI]|[xX][tT][hH][lL])\\b"
    },
    {
      "name": "string.quoted.double.asm",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "include": "#string-character-escape"
        }
      ]
    },
    {
      "name": "string.quoted.single.asm",
      "begin": "'",
      "end": "'",
      "patterns": [
        {
          "include": "#string-character-escape"
        }
      ]
    }
  ],
  "repository": {
    "string-character-escape": {
      "name": "constant.character.escape.asm",
      "match": "\\\\(x[0-9A-Fa-f]{2}|u[0-9A-Fa-f]{4}|u\\{[0-9A-Fa-f]+\\}|[0-2][0-7]{0,2}|3[0-6][0-7]?|37[0-7]?|[4-7][0-7]?|.|$)"
    }
  }
}
